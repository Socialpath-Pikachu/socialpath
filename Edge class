package Socialpath;

public class Edge {
    Vertex toVertex;
    double fromVertexRep;
    Edge nextEdge;
	
    public Edge(){
	toVertex = null;
	fromVertexRep = 0;
	nextEdge = null;
    }
	
    public Edge(Vertex destination, Edge a){
	toVertex = destination;
	nextEdge = a;
    }
    
    public Edge(Vertex destination, double rep, Edge a){
	toVertex = destination;
        fromVertexRep = rep;
	nextEdge = a;
    }
}
