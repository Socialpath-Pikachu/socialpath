package Socialpath;

public class Edge <T extends Comparable<T>, N extends Comparable <N>> {
    Vertex<T,N> srcVertex;
    Vertex<T,N> toVertex;
    N fromVertexRep;
    Edge<T,N> nextEdge;
	
    public Edge(){
        srcVertex = null;
	toVertex = null;
	fromVertexRep = null;
	nextEdge = null;
    }
	
    public Edge(Vertex<T,N> destination, N rep, Edge<T,N> a){
	toVertex = destination;
        fromVertexRep = rep;
	nextEdge = a;
    }
}
